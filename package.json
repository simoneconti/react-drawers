{
  "name": "@simoneconti/react-drawers",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/@simoneconti/react-drawers.umd.cjs",
  "module": "./dist/@simoneconti/react-drawers.js",
  "exports": {
    ".": {
      "import": "./dist/@simoneconti/react-drawers.js",
