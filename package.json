    "@types/node": "^20.1.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "eslintConfig": {
    "extends": "@simoneconti/eslint-config"
